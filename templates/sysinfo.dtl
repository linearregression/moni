{% extends "base.dtl" %}

<title>{% block title %}Nodes Overview{% endblock %}</title>

{% block body %}
<div class="container">
	<div class="well">
		<table class="table">  
        	<thead>  
          	  <tr>  
				  <th>Type</th>  
				  <th>Value</th>  
			  </tr>  
			</thead>
			<tbody>  
				{% for sysinfo, value in sysinfos %}		
				<tr>
					<td>{{sysinfo}}</td>
					<td>{{value}}</td>				
				</tr>
				{% endfor %}
			</tbody>
		</table>
	</div>
</div>
{% endblock %}